<h1>Add a new Grandma</h1>
<%= simple_form_for(@grandma) do |f| %>
  <%= f.input :first_name %>
  <%= f.input :last_name %>
  <%= f.input :location %>
  <%= f.input :description %>
  <%= f.input :skills %>
  <%= f.input :photos, as: :file, input_html: { multiple: true } %>
  <%= f.submit %>
<% end %>
